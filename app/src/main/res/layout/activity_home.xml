<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/homeView"
        tools:context=".HomeActivity">

    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/arrowup" android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="171dp"
            android:id="@+id/imageView0"
            android:layout_marginTop="85dp"/>



    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/arrowdown" android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="171dp"
            android:id="@+id/imageView1"
            android:layout_marginBottom="85dp"
            android:layout_marginTop="585dp"/>

</RelativeLayout>